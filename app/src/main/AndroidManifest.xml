<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.asus.mymusicplayer">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_010_wave"
        android:label="Sleep music"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NatureActivity"
            />
        <activity android:name=".NoiseActivity"
            />
        <activity android:name=".KidsActivity"
            />
        <activity android:name=".MeditationActivity"
            />

    </application>

</manifest>